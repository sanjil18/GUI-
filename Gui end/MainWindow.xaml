<Window x:Class="Gui_end.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gui_end"
        mc:Ignorable="d"
        Title="Login" Height="300" Width="400">
    <Grid>
        <Label Content="Registration Number:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,30,0,0"/>
        <TextBox Name="RegNoInput" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Margin="150,30,0,0"/>

        <Label Content="Password:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,80,0,0"/>
        <PasswordBox Name="PasswordInput" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Margin="150,80,0,0"/>

        <Button Content="Login" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="50,150,0,0" Click="LoginButton_Click"/>
        <Button Content="Signup" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="200,150,0,0" Click="SignUpbutton"/>
    </Grid>
</Window>